@import '/static/css/form.wxss';
@import "/static/css/ui.wxss";
.container{
    padding-bottom:100rpx;
}
.single_item{
    margin-top:0rpx;
}
.single_item .name_box{
    padding:35rpx 30rpx;
    /*border-bottom: 2px solid #EEEEEE;*/
}
.name_box .left_part image{
    width:80rpx;
    height:80rpx;
    border-radius:50%;
    margin-right:15rpx;
}
.name_box .state{
    font-size:26rpx;
}
.single_item .process{
    border-radius:6rpx;
    padding:30rpx;
    margin:0 30rpx;
    background:#f4f4f4;
}
.process_box{
    padding-bottom:30rpx;
}
.single_item .process .part_one image{
    width:38rpx;
    height:38rpx;
    margin-right:10rpx;
}
 .single_item .process .part_one .icon_more{
  width:39rpx;
  height:36rpx;
  margin-right:0;
  margin-top:6rpx;
}
.connect_type{
    padding:20rpx 30rpx;
    position:fixed;
    bottom:0;
    left:0;
    right:0;
}
.connect_type view{
    width:150rpx;
    height:60rpx;
    color:#FF8811;
    border:1px solid #FF8811;
    border-radius:10rpx;
    text-align:center;
    line-height:60rpx;
    font-size:32rpx;
}
.connect_type view:nth-child(2){
    margin:0 20rpx;
}
.connect_type view:nth-child(3){
    color:#fff;
    background:#FF8811;
}

.map_box{
  width: 100%;
  height: 860rpx;
  top: 35px;
  bottom: 90px;
  left: 0px;
  right: 0px;
}
#navi_map{
  width: 100%;
  height: 100%;
}

.price_box{
    padding:30rpx 1rpx 0rpx;
}
.price_box view{
    color:#FF655E;
}
.bake_box{
  padding: 19rpx 3rpx 0rpx;
}

.title{
    height:103rpx;
    padding:0 30rpx;
    border-bottom:1rpx solid #D9D9D9;
    border-top-left-radius: 10rpx;
    border-top-right-radius: 10rpx;
}
.title view{
    font-size:33rpx;
}
.title image{
    width:27rpx;
    height:27rpx;
}
.modal-content{
    overflow:auto;
    margin-bottom:60rpx;
}


.order_content{
    margin:20rpx 20rpx 0;
    border-radius: 6rpx;
}
.position_box{
    padding:0 20rpx;
}
.part_one .item_line{
    border-bottom: 2rpx solid #EEEEEE;
    height:99rpx;
}
.part_one .item_line image{
    width:38rpx;
    height:38rpx;
    margin-right:14rpx;
}
.position_box .part_two image{
    width:47rpx;
    height:47rpx;
    margin-left:30rpx;
}
.select_time{
    padding:0 20rpx;
    height:99rpx;
    border-bottom: 2rpx solid #EEEEEE;
}
.select_time image{
    width:38rpx;
    height:38rpx;
    margin-right:14rpx;
}

 .start-time-picker{
   width: 60%;
 }
 .start-time-picker picker{
   margin:0 auto;
}
 .start-time-picker2{
  margin-left: 19rpx;
 }

  .start-time-picker2 picker{
   margin:0 auto;
}
 .form_button_size{
    width:10%;
    height:9%;
}

.carPlate{
  color:#FFF;
  background:#c0c0c0;
  padding:6rpx 6rpx;
  font-weight:blod;
  font-size:10pt;
}

.toggle{
 width:73rpx;
 height:53rpx;
}

.center{
  justify-content: center;
}